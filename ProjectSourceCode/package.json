{
  "name": "parking-app",
  "version": "1.0.0",
  "description": "Parking application",
  "main": "src/index.js",
  "scripts": {
    "prestart": "npm install",
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "mocha src/test",
    "testandrun": "npm run prestart && npm run test && npm start",
    "seed_lots": "docker-compose exec -T db sh -c 'psql -U \"$POSTGRES_USER\" -d \"$POSTGRES_DB\" -f /docker-entrypoint-initdb.d/01_insert.sql'",
    "render-build": "npm run prestart && npm run seed_lots"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "express-handlebars": "^7.1.2",
    "express-session": "^1.17.3",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.0",
    "pg-promise": "^10.11.1",
    "connect-pg-simple": "^8.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "mocha": "^10.2.0",
   "chai": "^4.3.7",
   "chai-http": "^4.3.0"
  }
}